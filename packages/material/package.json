{
    "name": "@wq/material",
    "version": "1.3.0-dev",
    "description": "React + Material UI renderer for use with @wq/app",
    "main": "dist/index.js",
    "module": "dist/index.es.js",
    "scripts": {
        "test": "cd ../../ && npm run jest packages/material -- --moduleDirectories node_modules/@wq/react/node_modules node_modules",
        "build": "cd ../../ && npm run rollup -- -c packages/material/rollup.config.js",
        "prettier": "cd ../../ && npm run prettier -- --write packages/material/{,src/,src/*/,src/*/*/}*.js{,on}",
        "lint": "cd ../../ && npm run eslint packages/material/{,src/,src/*/,src/*/*/}*.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/wq/wq.app.git"
    },
    "keywords": [
        "wq",
        "react",
        "material",
        "material-ui"
    ],
    "author": "S. Andrew Sheppard",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/wq/wq.app/issues"
    },
    "homepage": "https://wq.io/wq.app",
    "dependencies": {
        "@material-ui/core": "^4.9.4",
        "@material-ui/icons": "^4.9.1",
        "@material-ui/lab": "^4.0.0-alpha.44",
        "@wq/react": "^1.3.0-alpha.0",
        "formik-material-ui": "^2.0.0-beta.1"
    }
}
