{
    "name": "@wq/material-web",
    "version": "3.0.0-alpha.1",
    "description": "Web bindings for @wq/material",
    "type": "module",
    "main": "index.js",
    "scripts": {
        "test": "cd ../../ && npm run jest packages/material-web",
        "build": "npm run babel && npm run prettier",
        "babel": "cd ../../ && npm run babel -- packages/material-web/src --out-dir packages/material-web/",
        "prettier": "cd ../../ && npm run prettier -- --write packages/material-web/",
        "lint": "cd ../../ && npm run eslint packages/material-web/{,src/,src/*/,src/*/*/}*.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/wq/material.git",
        "directory": "packages/material-web"
    },
    "keywords": [
        "wq",
        "react",
        "material",
        "material-ui"
    ],
    "author": "S. Andrew Sheppard",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/wq/material/issues"
    },
    "homepage": "https://material.wq.io/@wq/material-web",
    "dependencies": {
        "@wq/react": "^3.0.0-alpha.0"
    },
    "peerDependencies": {
        "@emotion/react": "*",
        "@emotion/styled": "*",
        "@mui/icons-material": "*",
        "@mui/material": "*",
        "react": "*",
        "react-dom": "*"
    },
    "devDependencies": {
        "@mui/icons-material": "^7.3.5",
        "@mui/material": "^7.3.5"
    }
}
